{
  "name": "mern-testing-assignment",
  "version": "1.0.0",
  "description": "Testing and Debugging MERN Application - Week 6 Assignment",
  "main": "index.js",
  "scripts": {
    "install-all": "npm install && cd client && npm install && cd ../server && npm install",
    "test": "jest --coverage",
    "test:unit": "jest --testPathPattern='tests/unit' --coverage",
    "test:integration": "jest --testPathPattern='tests/integration' --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:watch": "jest --watch",
    "test:client": "cd client && npm test",
    "test:server": "cd server && npm test",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm start",
    "setup-test-db": "node server/scripts/setup-test-db.js"
  },
  "keywords": [
    "mern",
    "testing",
    "jest",
    "react",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.5",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mongodb-memory-server": "^9.1.3",
    "supertest": "^6.3.3"
  }
}
